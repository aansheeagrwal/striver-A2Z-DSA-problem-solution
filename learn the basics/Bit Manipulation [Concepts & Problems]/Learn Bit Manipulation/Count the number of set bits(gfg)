class Solution{
    public:
    // Function to return sum of count of set bits in the integers from 1 to n.
    int countSetBits(int n) {
        int count = 0;
        for (int i = 1; i <= n; i++) {
            count += __builtin_popcount(i); // Counts set bits using built-in function
        }
        return count;
    }
};
